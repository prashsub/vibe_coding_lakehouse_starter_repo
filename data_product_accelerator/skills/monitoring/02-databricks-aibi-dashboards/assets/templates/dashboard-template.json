{
  "version": 1,
  "displayName": "Dashboard Template",
  "datasets": [
    {
      "name": "ds_example",
      "query": "SELECT CURRENT_TIMESTAMP() AS timestamp, 100 AS value",
      "parameters": [
        {
          "keyword": "time_range",
          "dataType": "DATETIME_RANGE",
          "defaultSelection": {
            "range": {
              "min": { "dataType": "DATETIME", "value": "now-30d/d" },
              "max": { "dataType": "DATETIME", "value": "now/d" }
            }
          }
        }
      ]
    }
  ],
  "pages": [
    {
      "name": "page_main",
      "displayName": "Main Page",
      "pageType": "PAGE_TYPE_DEFAULT",
      "layout": [
        {
          "widget": {
            "name": "widget_kpi",
            "queries": [
              { "name": "ds_example" }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "value",
                  "displayName": "Value",
                  "format": {
                    "type": "number-plain"
                  }
                }
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 2, "height": 1 }
        }
      ]
    }
  ]
}
