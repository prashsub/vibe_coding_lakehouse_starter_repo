{
  "datasets": [
    {
      "name": "customer_kpis",
      "displayName": "Customer KPIs",
      "queryLines": [
        "SELECT COUNT(DISTINCT user_id) as total_customers, SUM(CASE WHEN is_business THEN 1 ELSE 0 END) * 100.0 / COUNT(*) as business_pct FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.dim_user WHERE is_current = true"
      ]
    },
    {
      "name": "customer_segments",
      "displayName": "Customer Segments",
      "queryLines": [
        "SELECT \n",
        "  CASE \n",
        "    WHEN booking_count >= 5 THEN 'VIP (5+ bookings)'\n",
        "    WHEN booking_count >= 3 THEN 'Loyal (3-4 bookings)'\n",
        "    WHEN booking_count >= 2 THEN 'Returning (2 bookings)'\n",
        "    ELSE 'New (1 booking)'\n",
        "  END as segment,\n",
        "  COUNT(DISTINCT user_id) as customer_count,\n",
        "  SUM(total_spend) as total_revenue,\n",
        "  AVG(total_spend) as avg_spend_per_customer\n",
        "FROM (\n",
        "  SELECT \n",
        "    f.user_id,\n",
        "    COUNT(*) as booking_count,\n",
        "    SUM(f.total_amount) as total_spend\n",
        "  FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail f\n",
        "  WHERE f.check_in_date >= :start_date \n",
        "    AND f.check_in_date <= :end_date\n",
        "  GROUP BY 1\n",
        ") customer_summary\n",
        "GROUP BY 1\n",
        "ORDER BY 3 DESC"
      ],
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now-1y/y"
                }
              ]
            }
          }
        },
        {
          "displayName": "end_date",
          "keyword": "end_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now/d"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "business_vs_leisure",
      "displayName": "Business vs Leisure",
      "queryLines": [
        "SELECT \n",
        "  CASE \n",
        "    WHEN u.is_business THEN 'Business' \n",
        "    ELSE 'Leisure' \n",
        "  END as segment,\n",
        "  COUNT(DISTINCT f.booking_id) as bookings,\n",
        "  SUM(f.total_amount) as revenue,\n",
        "  AVG(f.total_amount) as avg_booking_value,\n",
        "  AVG(f.nights_booked) as avg_stay_length\n",
        "FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail f\n",
        "JOIN prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.dim_user u \n",
        "  ON f.user_id = u.user_id \n",
        "  AND u.is_current = true\n",
        "WHERE f.check_in_date >= :start_date \n",
        "  AND f.check_in_date <= :end_date\n",
        "GROUP BY 1"
      ],
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now-1y/y"
                }
              ]
            }
          }
        },
        {
          "displayName": "end_date",
          "keyword": "end_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now/d"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "customers_by_country",
      "displayName": "Customers by Country",
      "queryLines": [
        "SELECT u.country, COUNT(DISTINCT u.user_id) as customer_count, COUNT(DISTINCT f.booking_id) as bookings, SUM(f.total_amount) as revenue FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.dim_user u LEFT JOIN prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail f ON u.user_id = f.user_id AND f.check_in_date >= :start_date AND f.check_in_date <= :end_date WHERE u.is_current = true GROUP BY 1 ORDER BY 4 DESC LIMIT 15"
      ],
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now-1y/y"
                }
              ]
            }
          }
        },
        {
