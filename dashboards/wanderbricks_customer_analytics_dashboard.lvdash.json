    {
      "name": "new_vs_returning",
      "displayName": "New vs Returning",
      "queryLines": [
        "SELECT DATE_TRUNC('month', f.check_in_date) as month, COUNT(DISTINCT CASE WHEN customer_cohort.first_booking_date = f.check_in_date THEN f.user_id END) as new_customers, COUNT(DISTINCT CASE WHEN customer_cohort.first_booking_date < f.check_in_date THEN f.user_id END) as returning_customers FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail f JOIN (SELECT user_id, MIN(check_in_date) as first_booking_date FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail GROUP BY 1) customer_cohort ON f.user_id = customer_cohort.user_id WHERE f.check_in_date >= :start_date AND f.check_in_date <= :end_date GROUP BY 1 ORDER BY 1"
      ],
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
