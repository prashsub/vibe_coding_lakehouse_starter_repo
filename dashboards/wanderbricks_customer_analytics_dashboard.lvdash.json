                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "booking_frequency_distribution",
      "displayName": "Booking Frequency Distribution",
      "queryLines": [
        "SELECT CASE WHEN booking_count = 1 THEN '1 booking' WHEN booking_count = 2 THEN '2 bookings' WHEN booking_count BETWEEN 3 AND 4 THEN '3-4 bookings' WHEN booking_count BETWEEN 5 AND 9 THEN '5-9 bookings' ELSE '10+ bookings' END as frequency_bucket, COUNT(DISTINCT user_id) as customer_count FROM (SELECT user_id, COUNT(*) as booking_count FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail WHERE check_in_date >= :start_date AND check_in_date <= :end_date GROUP BY 1) booking_freq GROUP BY 1 ORDER BY 2 DESC"
      ],
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now-1y/y"
                }
              ]
            }
          }
        },
        {
          "displayName": "end_date",
          "keyword": "end_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now/d"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "lead_time_by_segment",
      "displayName": "Lead Time by Segment",
      "queryLines": [
        "SELECT CASE WHEN u.is_business THEN 'Business' ELSE 'Leisure' END as segment, AVG(f.days_between_booking_and_checkin) as avg_lead_time, PERCENTILE_APPROX(f.days_between_booking_and_checkin, 0.5) as median_lead_time FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail f JOIN prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.dim_user u ON f.user_id = u.user_id AND u.is_current = true WHERE f.check_in_date >= :start_date AND f.check_in_date <= :end_date GROUP BY 1"
      ],
      "parameters": [
        {
          "displayName": "start_date",
          "keyword": "start_date",
          "dataType": "DATE",
          "defaultSelection": {
            "values": {
              "dataType": "DATE",
              "values": [
                {
                  "value": "now-1y/y"
                }
