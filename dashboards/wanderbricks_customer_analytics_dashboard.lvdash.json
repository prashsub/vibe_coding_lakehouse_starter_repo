    {
      "name": "destination_preferences_by_segment",
      "displayName": "Destination Preferences by Segment",
      "queryLines": [
        "SELECT CASE WHEN u.is_business THEN 'Business' ELSE 'Leisure' END as segment, d.destination, COUNT(DISTINCT f.booking_id) as bookings, SUM(f.total_amount) as revenue FROM prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.fact_booking_detail f JOIN prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.dim_user u ON f.user_id = u.user_id AND u.is_current = true JOIN prashanth_subrahmanyam_catalog.dev_prashanth_subrahmanyam_wanderbricks_gold.dim_destination d ON f.destination_id = d.destination_id WHERE f.check_in_date >= :start_date AND f.check_in_date <= :end_date GROUP BY 1, 2 ORDER BY 1, 4 DESC LIMIT 30"
